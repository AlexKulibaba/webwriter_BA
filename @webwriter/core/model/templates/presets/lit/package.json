{
  "$schema": "https://raw.githubusercontent.com/webwriter-app/webwriter/main/webwriterpackage.json",
  "name": "ww-lit-preset",
  "version": "0.1.0",
  "scripts": {
    "prepublishOnly": "npx @webwriter/build",
    "dev": "npx @webwriter/build dev"
  },
  "dependencies": {
    "lit": "^3",
    "@webwriter/lit": "^2"
  },
  "exports": {
    "./widgets/---name---.*": {
      "source": "./widgets/---name---.ts",
      "default": "./dist/widgets/---name---.*"
    }
  }
}