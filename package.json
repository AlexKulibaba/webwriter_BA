{
  "name": "webwriter",
  "version": "0.2.0",
  "description": "Web-based authoring tool for open explorables",
  "scripts": {
    "dev-desktop": "npm run -w @webwriter/app-desktop dev",
    "dev-docs": "bundle exec jekyll serve -s ./docs",
    "lerna": "lerna",
    "publish": "lerna publish --no-private",
    "createbinaries": "node ./scripts/createbinaries.js",
    "preparestatic": "node ./scripts/preparestatic.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salmenf/webwriter.git"
  },
  "keywords": [
    "edtech",
    "education",
    "authoring",
    "explorable",
    "open",
    "web",
    "multimedia",
    "interactive"
  ],
  "author": "Frederic Salmen <frederic@fsalmen.de>",
  "license": "UNLICENSED",
  "workspaces": ["@webwriter/*"],
  "devDependencies": {
    "fs-extra": "^10.1.0",
    "lerna": "^5.6.1",
    "npm": "^8.19.1",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0"
  }
}
